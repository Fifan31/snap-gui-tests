        <Procedure id="_1l3q" name="open_product_dialog">
          <ComponentWaiter client="$(client)"
                           component="dialogESA_S-3_Toolbox_-_Open_Data_Product(s)"
                           id="_1l0r"/>
        </Procedure>
        <Procedure id="_1lAF" name="preferences_dialog">
          <ComponentWaiter client="$(client)"
                           component="dialogESA_S-3_Toolbox_Preferences_-_UI_Behavior"
                           id="_1l73"/>
        </Procedure>
        <Procedure id="_1oWp" name="product_is_opened">
          <comment>@teststep Wait if product appears.</comment>
          <TryStep id="_1lJj" name="click_OK_in_Warning_dialog">
            <ProcedureCall id="_1lJo" procedure="wait.appear.warning_dialog"/>
            <ProcedureCall id="_1lJn"
                           procedure="do_click.warning_dialog.button_ok"/>
            <CatchSequence exception="TestException" id="_1lJm" maxerror="0">
              <ProcedureCall id="_1oWq" procedure="check.product.not_opened"/>
              <IfSequence id="_1oWr"
                          test="&#34;$(not_opened)&#34;==&#34;true&#34;">
                <TryStep id="_1oWs">
                  <ComponentWaiter client="VISAT"
                                   component="mainFrameESA_S-3_Toolbox.ProductsToolView.Control.tree"
                                   errorlevel="0" id="_1oWt" raise="false"/>
                  <CatchSequence exception="TestException" id="_1oWu"
                                 maxerror="0"/>
                </TryStep>
                <ElseSequence id="_1oW+">
                  <TryStep id="_1oW-" name="click_OK_in_Warning_dialog">
                    <ProcedureCall id="_1oX0"
                                   procedure="wait.appear.warning_dialog"/>
                    <ProcedureCall id="_1oX1"
                                   procedure="do_click.warning_dialog.button_ok"/>
                    <CatchSequence exception="TestException" id="_1oX2"
                                   maxerror="0">
                      <ProcedureCall id="_1oX3"
                                     procedure="check.product.not_opened"/>
                      <IfSequence id="_1oX4"
                                  test="&#34;$(not_opened)&#34;==&#34;true&#34;">
                        <TryStep id="_1oX5">
                          <ComponentWaiter client="VISAT"
                                           component="mainFrameESA_S-3_Toolbox.ProductsToolView.Control.tree"
                                           errorlevel="0" id="_1oX6"
                                           raise="false"/>
                          <CatchSequence exception="TestException" id="_1oX7"
                                         maxerror="0"/>
                        </TryStep>
                      </IfSequence>
                      <ProcedureCall id="_1oX8"
                                     procedure="check.product.not_opened"/>
                      <IfSequence id="_1oX9"
                                  test="&#34;$(not_opened)&#34;==&#34;true&#34;">
                        <TryStep id="_1oXA">
                          <ComponentWaiter client="VISAT"
                                           component="mainFrameESA_S-3_Toolbox.ProductsToolView.Control.tree"
                                           errorlevel="0" id="_1oXB"
                                           raise="false"/>
                          <CatchSequence exception="TestException" id="_1oXC"
                                         maxerror="0"/>
                        </TryStep>
                      </IfSequence>
                    </CatchSequence>
                  </TryStep>
                </ElseSequence>
              </IfSequence>
            </CatchSequence>
          </TryStep>
        </Procedure>
        <Procedure id="_1l3Z" name="product_modified_dialog">
          <IfSequence id="_1oTR" test="${qftest:windows}">
            <ComponentWaiter client="$(client)"
                             component="dialogESA_S-3_Toolbox_-_Product_Modified"
                             id="_1l9o" timeout="100"/>
            <ElseifSequence id="_1oTS" name="MAC?"
                            test="'${system:os.name}' .find('Mac OS X')&gt;=0"/>
            <ElseSequence id="_1oTT">
              <ComponentWaiter client="$(client)"
                               component="dialogESA_S-3_Toolbox_-_Product_Modified"
                               id="_1oWl" timeout="100"/>
            </ElseSequence>
          </IfSequence>
        </Procedure>
        <Procedure id="_1lB6" name="dialog">
          <ComponentWaiter client="$(client)" component="Dialog" id="_1lB8"/>
        </Procedure>
        <Procedure id="_1lBF" name="new_pin_dialog">
          <ComponentWaiter client="$(client)" component="New_Pin" id="_1lGd"/>
        </Procedure>
        <Procedure id="_1k-o" name="view">
          <variable name="prod_nr"/>
          <variable name="band_name"/>
          <comment>@teststep Check view title</comment>
          <ComponentWaiter client="$(client)"
                           component="mainFrameESA_S-3_Toolbox.intFrame_band"
                           id="_1k-V" raise="false"/>
        </Procedure>
        <Procedure id="_1lK6" name="dialogESA_S-3_Toolbox_Question">
          <ComponentWaiter client="$(client)"
                           component="dialogESA_S-3_Toolbox_-_Question"
