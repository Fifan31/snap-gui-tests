        <Procedure id="_1lB9" name="computing_statistics_progress monitor">
          <ComponentWaiter absence="true" client="$(client)"
                           component="dialogComputing_Statistics" id="_1l9V"
                           raise="false" timeout="3000000"/>
        </Procedure>
      </Package>
    </Package>
  </PackageRoot>
  <ExtraSequence id="_4"/>
  <WindowList id="_5"/>
</RootStep>
