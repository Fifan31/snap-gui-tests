        </Procedure>
        <Procedure id="_1mQH" name="warning_dialog">
          <IfSequence id="_1mMl" test="${qftest:windows}">
            <ComponentWaiter client="$(client)"
                             component="dialogESA_S-3_Toolbox_-_Warning"
                             errorlevel="0" id="_1mJU" raise="false"
                             resvarname="warning_dialog" timeout="500"/>
            <ElseifSequence id="_1mMm" name="MAC?"
                            test="'${system:os.name}' .find('Mac OS X')&gt;=0"/>
            <ElseSequence id="_1mMn">
              <ComponentWaiter client="$(client)"
                               component="dialogESA_S-3_Toolbox_-_Error"
                               id="_1mMi" raise="false"
                               resvarname="warning_dialog" timeout="500"/>
            </ElseSequence>
          </IfSequence>
        </Procedure>
      </Package>
      <Package id="_1lBA" name="disappear">
