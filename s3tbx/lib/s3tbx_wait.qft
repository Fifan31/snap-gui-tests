    <Package id="_1lB5" name="wait">
      <Package id="_1lBB" name="appear">
        <Procedure id="_1lK4" name="import_dialog">
          <ComponentWaiter client="$(client)"
                           component="dialogESA_S-3_Toolbox_-_Import_Product"
