          <ProcedureCall id="_1pEQ"
                         procedure="do_click.image_view.start_point_drag_to_end_point">
            <variable name="view_name">$(view_name)</variable>
            <variable name="start_x">$(x0)</variable>
            <variable name="start_y">$(y0)</variable>
