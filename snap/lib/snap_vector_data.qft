        <Procedure id="_1opE" name="polyline">
          <variable name="view_name"/>
          <variable name="x0"/>
          <variable name="y0"/>
          <variable name="x1"/>
          <variable name="y1"/>
          <variable name="x2"/>
          <variable name="y2"/>
          <variable name="x3"/>
          <variable name="y3"/>
          <ProcedureCall id="_1pET"
                         procedure="do_click.main_toolbar.tools.draw_polyline"/>
          <ProcedureCall id="_1pES"
