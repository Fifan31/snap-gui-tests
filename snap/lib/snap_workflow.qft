                  </ProcedureCall>
                  <ElseSequence id="_1oHW">
                    <ServerScriptStep id="_1oHX" name="error">
                      <code>rc.logError("Layer manager is empty.")</code>
                    </ServerScriptStep>
                  </ElseSequence>
                </IfSequence>
                <ProcedureCall id="_1lHU" procedure="close.dialog">
                  <comment>@teststep Close Layer Manager</comment>
