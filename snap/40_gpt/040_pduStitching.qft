<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE RootStep>
<RootStep id="_0" name="root" version="4.0.9">
  <include>qfs.qft</include>
  <include>gpt_main.qft</include>
  <variable name="output_folder">gpt_pdustitching</variable>
  <variable name="sleep_time">1000</variable>
  <variable name="gpf_op">PduStitching</variable>
  <variable name="xml_file_dir">pdustitching</variable>
  <TestSet id="_5G1" name="040_pdu_stitching"
           reportname="040 PduStitching operator">
    <DependencyReference id="_1pP1" reference="start_application_for_gpt">
      <variable name="output_dir">$(output_folder)</variable>
    </DependencyReference>
    <TestSet id="_9V3" name="pduStitching" postdelay="2000"
             reportname="run PduStitching operator, check target product">
      <DataDriver id="_9V5"
                  loopreportname="$(xml_file) $(dataInDir)/$(fileName)"
                  name="dataTable">
        <ProcedureCall id="_1l4d" procedure="tables.pdustitching"/>
      </DataDriver>
      <TestCase id="_9Xo" name="pduStitching"
                reportname="PduStitching Op $(dataInDir)/$(fileName)">
        <ProcedureCall id="_1pP2" procedure="test_sequence_pduStitching"/>
      </TestCase>
    </TestSet>
  </TestSet>
  <PackageRoot id="_4"/>
  <ExtraSequence id="_5"/>
  <WindowList id="_6"/>
</RootStep>
