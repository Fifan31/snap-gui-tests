                  name="dataTable">
        <ProcedureCall id="_1l4d" procedure="tables.smac"/>
      </DataDriver>
      <TestCase id="_9Xo" name="smac"
                reportname="run SmacOp $(xml_file)$(parameters_file)">
        <ProcedureCall id="_1pP2" procedure="test_sequence"/>
      </TestCase>
    </TestSet>
  </TestSet>
  <PackageRoot id="_4"/>
  <ExtraSequence id="_5"/>
  <WindowList id="_6"/>
</RootStep>
