                         procedure="utils.waiting_for_processierung_else_terminate_process">
            <variable name="client_name">$(gpt_client_name)</variable>
          </ProcedureCall>
          <ServerScriptStep disabled="true" id="_1qWq" name="print">
            <code>print "*** gpt_client_nr = $(wv)"  </code>
