                <ProcedureCall id="_1qWZ" procedure="path.param_file_dir"/>
                <SetGlobalStep id="_1qWa" varname="new_param">
                  <default>$(gpf_op) -p $(param_file_dir)$(parameters_file) $(param) - -PpatchedFile=$(output_data_directory)$(parameters_file).N1 -e -Sn1=$(template_N1_file)</default>
