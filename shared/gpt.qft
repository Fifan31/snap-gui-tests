          <TestStep id="_1qZH" name="compose_command_line ">
            <SetGlobalStep id="_1qZG" varname="gpt_commando">
              <default>$(gpt_calling) $(new_param) $(input)</default>
            </SetGlobalStep>
            <ServerScriptStep id="_1qZY" name="print">
              <code>print "*** gpt_commando = $(gpt_commando)"  </code>
