      <Procedure id="_1qR8" name="run_pconvert_check_output_file">
        <variable name="application"/>
        <IfSequence id="_1qTq" postdelay="2000"
                    test="&#34;$(supported_format)&#34;==&#34;true&#34;">
          <TestStep id="_1qVt" name="set_processing_parameters">
            <ProcedureCall id="_1qZQ"
                           procedure="variables.variable_increase_on_1">
              <variable name="var_int">$(sleep_time)</variable>
            </ProcedureCall>
            <ProcedureCall disabled="true" id="_1qXz"
                           procedure="variables.variable_increase_on_1">
              <variable name="var_int">100</variable>
            </ProcedureCall>
            <ProcedureCall id="_1qST" procedure="utils.pconvert_calling"/>
            <ProcedureCall id="_1qX-" procedure="path.main_path"/>
            <ProcedureCall id="_1qY0"
                           procedure="utils.set_working_directory"/>
            <TestStep id="_1qY1" name="set_iput">
              <ProcedureCall id="_1qYK"
                             procedure="path.source_data_directory">
                <variable name="data_type">$(dataInDir)</variable>
              </ProcedureCall>
              <SetGlobalStep id="_1qYH" varname="input">
                <default>$(data_source_directory)$(fileName)</default>
              </SetGlobalStep>
              <ServerScriptStep id="_1qYI" name="print">
                <code>print "*** gpt_input = $(input)"  </code>
              </ServerScriptStep>
            </TestStep>
            <ProcedureCall id="_1qYJ" procedure="utils.gpt_client_name">
              <variable name="client_nr">$(wv)</variable>
            </ProcedureCall>
            <ProcedureCall id="_1qSb" procedure="path.output_data_directory">
              <variable name="output_dir">$(output_folder)</variable>
            </ProcedureCall>
            <TestStep id="_1qXk" name="compose_command_line ">
              <comment>@teststep Set Command Line</comment>
              <SetGlobalStep id="_1qXl" varname="pconvert_commando">
                <default>$(pconvert_calling) $(of) -o $(output_data_directory) $(input)</default>
              </SetGlobalStep>
              <SetGlobalStep disabled="true" id="_1qZ0"
                             varname="pconvert_commando">
                <default>$(pconvert_calling) $(of) -o $(output_pconvert_file).$(fr) $(input)</default>
              </SetGlobalStep>
              <IfSequence id="_1qXm" test="${qftest:windows}">
                <ElseSequence id="_1qXn"/>
              </IfSequence>
              <ServerScriptStep disabled="true" id="_1qYz" name="print">
                <code>print "----- pconvert_commando = $(pconvert_commando)------"</code>
              </ServerScriptStep>
            </TestStep>
          </TestStep>
          <ProcedureCall id="_1qZV" procedure="utils.terminate_process">
            <variable name="client_name">$(gpt_client_name)</variable>
          </ProcedureCall>
          <ProcedureCall id="_1qZW" procedure="utils.terminate_process">
            <variable name="client_name">$(gpt_client_name)</variable>
          </ProcedureCall>
          <TestStep id="_1qYq" name="run_operator">
            <comment>$(pconvert_commando)</comment>
            <ShellClientStarter client="$(gpt_client_name)"
                                command="$(pconvert_commando)"
                                directory="$(folder)" id="_1qYt"
                                postdelay="20000"/>
            <ProcedureCall id="_1qYx" postdelay="1000"
                           procedure="run-log.log_client_output">
              <variable name="client_name">$(gpt_client_name)</variable>
            </ProcedureCall>
            <ProcedureCall id="_1qYy"
                           procedure="utils.waiting_for_processierung_else_terminate_process">
              <variable name="client_name">$(gpt_client_name)</variable>
            </ProcedureCall>
          </TestStep>
          <TestStep id="_1qS7" name="check_output">
            <ServerScriptStep disabled="true" id="_1qUb" name="print">
              <code>print "----- output file = $(outfile)------"</code>
            </ServerScriptStep>
            <ProcedureCall id="_1qSF"
                           procedure="check_external_file.file_exists_setGlobal">
              <variable name="fileName">$(outfile)</variable>
            </ProcedureCall>
            <IfSequence id="_1qSG"
                        test="&#34;$(fileExists)&#34;==&#34;False&#34;">
              <TestStep id="_1qSH" name="wait" postdelay="6000"
                        predelay="2000">
                <ProcedureCall id="_1qSI"
                               procedure="utils.waiting_for_processierung_else_terminate_process">
                  <variable name="client_name">$(gpt_client_name)</variable>
                </ProcedureCall>
              </TestStep>
              <ProcedureCall id="_1qSJ"
                             procedure="check_external_file.file_exists_setGlobal">
                <variable name="fileName">$(outfile)</variable>
              </ProcedureCall>
              <IfSequence id="_1qSK"
                          test="&#34;$(fileExists)&#34;==&#34;False&#34;">
                <ProcedureCall id="_1qSL" procedure="run-log.logError">
                  <variable name="message">$(outfile) is not processed</variable>
                  <variable name="withScreenshots">false</variable>
                </ProcedureCall>
              </IfSequence>
            </IfSequence>
          </TestStep>
        </IfSequence>
        <ServerScriptStep disabled="true" id="_1qSM" name="print">
          <code>print "of = $(of)"</code>
        </ServerScriptStep>
      </Procedure>
