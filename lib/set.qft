                <default>-f NetCDF4-CF</default>
              </SetGlobalStep>
              <SetGlobalStep id="_1okx" varname="fr">
                <default>nc</default>
              </SetGlobalStep>
              <SetGlobalStep id="_1oky" varname="output_netcdf_cf">
