<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE RootStep>
<RootStep id="_0" name="root" version="4.0.1">
  <include>qfs.qft</include>
  <include>../lib/main.qft</include>
  <variable name="sleep_time">100</variable>
  <variable name="xml_file_dir">bandArithmetic</variable>
  <variable name="gpf_op">BandMaths</variable>
  <variable name="data_out_dir">gpt_math</variable>
  <TestSet id="_3VQ" name="040-01_bandMath"
           reportname="040-01 gpt Operator BandMaths">
    <Dependency id="_1l4X" name="start_application">
      <comment>@teststep Check if ESA S-3 Toolbox started else start </comment>
      <SetupSequence id="_1l4Y" name="start_s3tbx">
        <ProcedureCall id="_1l4Z"
                       procedure="preparation.environment_for_gpt_test">
          <variable name="out_dir">$(data_out_dir)</variable>
        </ProcedureCall>
        <ProcedureCall id="_A" procedure="start.snap.application"/>
      </SetupSequence>
      <CleanupSequence id="_8" name="exit_s3tbx">
        <ProcedureCall id="_B" procedure="stop.exit_s3tbx"/>
      </CleanupSequence>
    </Dependency>
    <TestSet id="_2rH" name="bandMaths_xml_file"
             reportname="run BandMaths Operator using xml or properties file">
      <comment>xml_file	parameters_file	dataInDir	fileName	param	dataInDir2	fileName2	other_formats	sourceOption	sourceOption2	group_id1	view_Id1	group_id2	view_Id2	bandName	bandName2	unit1	unit2	invalid_pixel_val	dataInDir3	fileName3	
</comment>
      <DataDriver id="_9V5"
                  loopreportname="$(xml_file) $(dataInDir)/$(fileName) $(dataInDir2)/$(fileName2)"
                  name="dataTable">
        <ExcelDataBinder excelfile="../tables/gpt_bandMaths.xls" id="_9V6"
                         name="dataTable" sheetname="requests"/>
      </DataDriver>
      <TestCase id="_9YC" name="bandmaths_using_xml_file"
                reportname="run Band Maths operator using xml file ">
        <ProcedureCall id="_1l4c" procedure="gpt.general_test_sequence"/>
      </TestCase>
    </TestSet>
  </TestSet>
  <PackageRoot id="_4"/>
  <ExtraSequence id="_5"/>
  <WindowList id="_6"/>
</RootStep>
